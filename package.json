        "twa:build-release": "npx @bubblewrap/cli build --release",
        "ios:setup": "node scripts/setup-ios.js",
        "ios:sync": "npx cap sync ios",
        "ios:copy": "npx cap copy ios",
        "ios:open": "npx cap open ios",
